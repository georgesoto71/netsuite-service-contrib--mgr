@using System
@using System.Reflection
@using StubGenerator.Common
@using StubGenerator.ClientStub
@inherits RazorLight.TemplatePage<Context<ClientTypeStub>>
@{ DisableEncoding = true; }
@{ string targetInterfaceName = Model.Data.Interface.Name; }
namespace @Model.Configuration.TargetNamespace
{
    public partial interface @targetInterfaceName
    {
    #pragma warning disable IDE1006 // Naming Styles

@foreach (var mthd in Model.Data.Interface.Methods)
{
@:        @mthd.ToString(Model.Configuration.TargetNamespace, null);
}
    #pragma warning restore IDE1006 // Naming Styles
    }

    public partial class NetSuitePortTypeClient: @targetInterfaceName
    {
@foreach (var mthd in Model.Data.GetWrapperMethods())
    {
@:      @(mthd.ToString(Model.Configuration.TargetNamespace, "public"))
@:      {
@:          var request = new @(mthd.WrappedParameterType.Name)() {
    @foreach (var prop in mthd.WrappedParameterProperties)
    {
        @:              @prop = @prop,
    }
@:          };
@:          return ((NetSuitePortType)this).@(mthd.Name)(request);
@:      }
@:
}
    }
}
